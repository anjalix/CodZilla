int count;  /* Count of numbers */
int min;    /* To store the minimum value */
int temp;   /* For use if required */
    min = 0;
    count = 0;
    while (count < 
7
)
    {
        scanf("%d", a);
        
if(count==0 || min>a) {min=a;}

        count++;
    }
    printf("%d\n", min);
    return(0);
}
